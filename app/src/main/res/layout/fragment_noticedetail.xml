<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout9"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".noticedetail.NoticeDetailFragment">

        <TextView
            android:id="@+id/noticedetail_tv_noticeNameLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="NoticeName"
            app:layout_constraintBottom_toTopOf="@+id/noticedetail_tv_noticeName"
            app:layout_constraintStart_toStartOf="@+id/noticedetail_tv_noticeName" />

        <TextView
            android:id="@+id/noticedetail_tv_noticeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:text="noticeName"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline9" />

        <TextView
            android:id="@+id/noticedetail_tv_memoLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="memo"
            app:layout_constraintBottom_toTopOf="@+id/noticedetail_tv_memo"
            app:layout_constraintStart_toStartOf="@+id/noticedetail_tv_memo" />

        <TextView
            android:id="@+id/noticedetail_tv_memo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:ems="10"
            android:text="This is memo~!~!"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/noticedetail_tv_noticeName"
            app:layout_constraintTop_toBottomOf="@+id/noticedetail_tv_noticeName" />

        <TextView
            android:id="@+id/noticedetail_tv_worksiteNameLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tv_worksiteNameLabel"
            app:layout_constraintBottom_toTopOf="@+id/noticedetail_tv_worksiteName"
            app:layout_constraintStart_toStartOf="@+id/noticedetail_tv_worksiteName" />

        <TextView
            android:id="@+id/noticedetail_tv_worksiteName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:ems="10"
            android:text="worksiteName"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="@+id/noticedetail_tv_memo"
            app:layout_constraintTop_toBottomOf="@+id/noticedetail_tv_memo" />

        <Button
            android:id="@+id/noticedetail_bt_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="ok"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/noticedetail_tv_worksiteName" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.15" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>